icon-holder {
  align-items: center;
  display: inline-flex;
  justify-content: center;
  height: 1.5rem;
  width: 1.5rem;
  position: relative;
  font-size: 1.4em;
  height: 1em;
  width: 1em;
  vertical-align: middle;
}

icon-holder > [icon] {
  font-size: 21px;
}

icon-holder svg {
  width: 1em;
  height: 1em;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

image-holder {
  display: block;
  position: relative;
  /*
		padding-top: y/x%;
	*/
}

image-holder > img {
  display: block;
  height: auto;
  width: 100%;
}

image-holder[fix] > img {
  bottom: 0;
  left: 0;
  position: absolute;
  right: 0;
  top: 0;
  height: 100%;
  width: 100%;
}

fullscreen-holder {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
}

fullscreen-holder[active] {
  opacity: 1;
  transition: opacity 0.1s ease-out;
}

fullscreen-holder:not([active]) {
  opacity: 0;
  pointer-events: none;
  z-index: -1;
  transition: z-index 0.1s step-end, opacity 0.1s ease-out;
}

tag {
  color: rgba(0, 0, 0, 0.870588);
  font-size: 1em;
  font-weight: 400;
  line-height: 1.7em;
  padding-left: 1em;
  padding-right: 1em;
  user-select: none;
  -webkit-tap-highlight-color: transparent;
  cursor: default;
  text-decoration: none;
  padding-top: 0.4em;
  padding-bottom: 0.4em;
  position: relative;
  background-color: #e0e0e0;
  border-radius: 1em;
  white-space: nowrap;
  width: fit-content;
}

tag + tag {
  margin-left: 0.7em;
}

segment {
  padding-left: 0.5em;
  padding-right: 0.5em;
  border-color: #5DADE2;
  background: #FFF;
  color: #5DADE2;
  height: 1.5em;
  line-height: 1.5em;
  -moz-appearance: none;
  -webkit-appearance: none;
  align-items: center;
  border: 1px solid transparent;
  border-radius: 3px;
  box-shadow: none;
  display: -webkit-inline-box;
  display: -ms-inline-flexbox;
  display: inline-flex;
  font-size: 1rem;
  height: 2.25em;
  justify-content: flex-start;
  line-height: 1.5;
  padding-bottom: calc(0.375em - 1px);
  padding-left: calc(0.625em - 1px);
  padding-right: calc(0.625em - 1px);
  padding-top: calc(0.375em - 1px);
  position: relative;
  vertical-align: top;
  -webkit-touch-callout: none;
  user-select: none;
  background-color: white;
  border-color: #dbdbdb;
  color: #363636;
  cursor: pointer;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  padding-left: 0.75em;
  padding-right: 0.75em;
  text-align: center;
  white-space: nowrap;
}

segment[active] {
  background: #5DADE2;
  color: #fff;
}

segment:hover:not([active]) {
  border-color: #5DADE2;
  color: #5DADE2;
}

info-graphic {
  position: relative;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

modal {
  display: block;
  opacity: 1;
  visibility: visible;
  overflow: visible;
  /*max-height: 640px;*/
  text-align: center;
  padding: 0;
  animation: none;
  direction: ltr;
  border: 0;
  font-size: 14px;
  line-height: 24px;
  border-radius: 5px;
  width: 293.75px;
  box-sizing: border-box;
  transition: top cubic-bezier(0, 1, 0.5, 1) 100ms;
  z-index: 99999999;
  background: #fff;
  box-shadow: 1px 0 0 #e6e6e6, -1px 0 0 #e6e6e6, 0 1px 0 #e6e6e6, 0 -1px 0 #e6e6e6, 0 3px 13px rgba(0, 0, 0, 0.08);
}

[row-layout] {
  align-items: center;
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  display: flex;
}

[row-layout] > label {
  align-items: center;
  display: flex;
  flex-basis: auto;
  flex-grow: 0;
  flex-shrink: 0;
  justify-content: center;
}

[row-layout] > item {
  align-items: center;
  display: flex;
  flex-basis: auto;
  flex-grow: 0;
  flex-shrink: 0;
  justify-content: center;
}

[row-layout] > label {
  flex-grow: 0;
}

[row-layout] > item {
  flex-grow: 1;
  margin-left: 20px;
}

[row-layout]:not(:last-child) {
  margin-bottom: unset;
}

[strip-layout] {
  height: 40px;
  text-align: center;
  outline: 0;
  line-height: 40px;
  max-height: 40px;
  box-sizing: border-box;
  overflow: hidden;
  transition: height 0.33s cubic-bezier(0, 1, 0.5, 1);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  border-top: 1px solid #e6e6e6;
}

[strip-layout] > [patch-layout] {
  flex-grow: 1;
  float: left;
  position: relative;
}

[strip-layout] > [patch-layout][active] {
  background: rgba(0, 0, 0, 0.05);
}

[strip-layout] > label {
  outline: 0;
  flex-grow: 1;
  text-align: center;
  display: inline-block;
  float: left;
  color: #393939;
  user-select: none;
}

[strip-layout] > label[active] {
  background: #f0f0f0;
}

[page-layout] {
  height: 100%;
  display: flex;
  flex-flow: column;
  transition: opacity 2s cubic-bezier(0.23, 1, 0.32, 1) 0ms, transform 1s cubic-bezier(0.23, 1, 0.32, 1) 0ms;
}

[page-layout]:after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0.1;
  z-index: -1;
  background: white;
}

[page-layout] > [page-component] {
  flex: 0 1 auto;
}

[page-layout] > component-main-content {
  flex: 1 1 auto;
  position: relative;
}

[page-layout][no-background] {
  background: white;
}

[page-layout][no-background]:after {
  content: unset;
}

[page-layout][light-background] {
  background: rgba(255, 255, 255, 0.4);
}

[side-by-side-layout] {
  justify-content: space-between;
  display: flex;
  position: relative;
}

[side-by-side-layout] > [left] {
  text-align: left;
}

[side-by-side-layout] > [right] {
  text-align: right;
}

[side-by-side-layout] > [versus] {
  text-align: center;
  display: block;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
