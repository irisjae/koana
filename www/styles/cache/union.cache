[match-edge] {
  position: absolute;
  height: auto;
  width: auto;
  display: block;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

[match-me] {
  /*== position-relative full-height full-width top-down*/
  position: relative;
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

[list] {
  will-change: scroll-position;
}

[scroll] {
  overflow-x: hidden;
  overflow-y: scroll !important;
  -webkit-overflow-scrolling: touch;
}

[scroll]::-webkit-scrollbar {
  display: none;
}

