  visibility: visible;
  overflow: visible;
  /*max-height: 640px;*/
  text-align: center;
  padding: 0;
  animation: none;
  direction: ltr;
  border: 0;
  font-size: 14px;
  line-height: 24px;
  border-radius: 5px;
  width: 293.75px;
  box-sizing: border-box;
  transition: top cubic-bezier(0, 1, 0.5, 1) 100ms;
  z-index: 99999999;
  background: #fff;
  box-shadow: 1px 0 0 #e6e6e6, -1px 0 0 #e6e6e6, 0 1px 0 #e6e6e6, 0 -1px 0 #e6e6e6, 0 3px 13px rgba(0, 0, 0, 0.08); }

[row-layout] {
  align-items: center;
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  display: flex; }

[row-layout] > label {
  align-items: center;
  display: flex;
  flex-basis: auto;
  flex-grow: 0;
  flex-shrink: 0;
  justify-content: center; }

[row-layout] > item {
  align-items: center;
  display: flex;
  flex-basis: auto;
  flex-grow: 0;
  flex-shrink: 0;
  justify-content: center; }

[row-layout] > label {
  flex-grow: 0; }

[row-layout] > item {
  flex-grow: 1;
  margin-left: 20px; }

[row-layout]:not(:last-child) {
  margin-bottom: unset; }

[strip-layout] {
  height: 40px;
  text-align: center;
  outline: 0;
  line-height: 40px;
  max-height: 40px;
  box-sizing: border-box;
  overflow: hidden;
  transition: height 0.33s cubic-bezier(0, 1, 0.5, 1);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  border-top: 1px solid #e6e6e6; }

[strip-layout] > [patch-layout] {
  flex-grow: 1;
  float: left;
  position: relative; }

[strip-layout] > [patch-layout][active] {
  background: rgba(0, 0, 0, 0.05); }

[strip-layout] > label {
  outline: 0;
  flex-grow: 1;
  text-align: center;
  display: inline-block;
  float: left;
  color: #393939;
  user-select: none; }

[strip-layout] > label[active] {
  background: #f0f0f0; }

[page-layout] {
  height: 100%;
  display: flex;
  flex-flow: column;
  transition: opacity 2s cubic-bezier(0.23, 1, 0.32, 1) 0ms, transform 1s cubic-bezier(0.23, 1, 0.32, 1) 0ms; }

[page-layout]:after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0.1;
  z-index: -1;
  background: white; }

[page-layout] > [page-component] {
  flex: 0 1 auto; }

[page-layout] > component-main-content {
  flex: 1 1 auto;
  position: relative; }

[page-layout][no-background] {
  background: white; }

[page-layout][no-background]:after {
  content: unset; }

[page-layout][light-background] {
  background: rgba(255, 255, 255, 0.4); }

[side-by-side-layout] {
  justify-content: space-between;
  display: flex;
  position: relative; }

[side-by-side-layout] > [left] {
  text-align: left; }

[side-by-side-layout] > [right] {
  text-align: right; }

[side-by-side-layout] > [versus] {
  text-align: center;
  display: block;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%); }

