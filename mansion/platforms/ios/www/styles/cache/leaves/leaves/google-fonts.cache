  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale; }

[info-layout] {
  background: transparent; }

[empty-layout] {
  /*== position-relative full-height full-width top-down*/
  position: relative;
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden; }

[main-content-layout]::after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0.1;
  z-index: -1;
  background: url(../imgs/3d_soccer15.gif); }

item-holder {
  padding: 10px;
  border-radius: 0;
  display: block; }

control-holder {
  display: flex;
  flex-grow: 1;
  margin-left: 20px;
  justify-content: flex-end; }

control-holder input {
  box-shadow: none !important; }

data-holder {
  display: flex;
  justify-content: flex-end;
  flex-grow: 1;
  margin-left: 20px; }

[data] {
  color: black;
  font-weight: 500; }

info-header[data] {
  font-weight: 600; }

info-header [data] {
  font-weight: 600; }

fullscreen-holder {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1000; }

fullscreen-holder[active] {
  opacity: 1;
  transition: opacity 0.1s ease-out; }

fullscreen-holder:not([active]) {
  opacity: 0;
  pointer-events: none;
  z-index: -1;
  transition: z-index 0.1s step-end, opacity 0.1s ease-out; }

box-highlight {
  display: block;
  width: 100%;
  height: 100%; }

box-highlight > * {
  width: 100%;
  display: block;
  position: relative; }

box-highlight > box-top {
  height: 56.25%; }

box-highlight > box-bottom {
  height: 43.75%; }

tag {
  padding-left: 0.5em;
  padding-right: 0.5em;
  border-color: #5DADE2;
  background: #fff;
  color: #5DADE2;
  height: 1.5em;
  line-height: 1.5em;
  /*&:hover, */ }

tag[active] {
  background: #5DADE2;
  color: #fff; }

tag:hover:not([active]) {
  border-color: #5DADE2;
  color: #5DADE2; }

tag + tag {
  margin-left: 0.3em; }

[nav-button] {
  color: white;
  font-weight: bold;
  vertical-align: sub;
  /*filter: drop-shadow(1px 1px 1px #ddd);*/ }

nav {
  border-bottom: solid #CCD1D1 1px;
  color: #FFFFFF;
  background-color: #075E54;
  transition: all 450ms cubic-bezier(0.23, 1, 0.32, 1) 0ms;
  box-sizing: border-box;
  font-family: Roboto, sans-serif;
  -webkit-tap-highlight-color: transparent;
  box-shadow: rgba(0, 0, 0, 0.117647) 0px 1px 6px, rgba(0, 0, 0, 0.117647) 0px 1px 4px;
  border-radius: 0px;
  position: relative;
  width: 100%;
  display: flex;
  padding-left: 24px;
  padding-right: 24px; }

nav > * {
  position: absolute;
  height: auto;
  width: auto;
  display: block;
  top: 50%;
  transform: translateY(-50%); }

nav > nav-buttons:first-child {
  left: 10px;
  text-align: left; }

nav > nav-title {
  z-index: -1;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
  text-align: center; }

nav > nav-buttons:last-child {
  right: 10px;
  text-align: right; }

rest {
  background: rgba(194, 194, 194, 0.2);
  flex: 1;
  width: 100%; }

[info-layout] + rest:before {
  content: '';
  height: 20px;
  background: rgba(194, 194, 194, 0.2);
  display: flex;
  margin-top: -20px; }

[row] {
  display: inline-flex;
  justify-content: inherit;
  width: 100%;
  position: relative; }

info-graphic [row] > * {
  width: 128px;
  position: relative; }

[grayed-header] {
  color: #000;
  line-height: 24px;
  position: relative; }

[grayed-header]:before {
  content: '';
  display: block;
  position: absolute;
  left: -15px;
  right: -15px;
  top: -10px;
  height: 44px;
  background: rgba(194, 194, 194, 0.2); }

[grayed-action]:before {
  content: '';
  display: block;
  position: absolute;
  left: -20px;
  right: -20px;
  top: -25px;
  height: 100px;
  background: rgba(194, 194, 194, 0.2); }

[info-table] > * {
  /*&:not(:last-child) {
			margin-bottom: 15px;
		}*/ }

[info-table] > * > label {
  flex-grow: 0 !important;
  /*padding-right: 30px;*/ }

[info-table] > * > label svg {
  transform: translateY(2px); }

[info-table] > * > control-holder > component-input-control {
  width: 100%; }

[info-table] > * h1 {
  margin: 0; }

[info-table] > * h2 {
  margin: 0; }

[info-table] > * h3 {
  margin: 0; }

[info-table] > * h4 {
  margin: 0; }

[info-table] > * h5 {
  margin: 0; }

[info-table] > * h6 {
  margin: 0; }

[info-table] > component-separator > hr {
  margin-top: -10px; }

[info-layout] {
  padding: 10px 15px 10px 15px;
  display: block; }

[info-layout] > *:not(:last-child) {
  margin-bottom: 35px; }

[info-layout] > a {
  border-color: #5DADE2;
  background: rgba(255, 255, 255, 0.5);
  color: #75c8ff; }

[info-layout] > component-separator > hr {
  margin-top: -20px; }

info-graphic {
  position: relative;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 10px 0px 10px 0px; }

info-area {
  display: block;
  background: transparent; }

info-area info-header {
  display: block; }

[main-content-layout] {
  height: 100%;
  display: flex;
  flex-flow: column; }

[main-content-layout] > component-nav {
  flex: 0 1 auto; }

[main-content-layout] > component-tabs {
  flex: 0 1 auto; }

[main-content-layout] > component-main-content {
