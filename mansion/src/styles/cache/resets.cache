html {
  background: white;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none; }

.box:not(:last-child) {
  margin-bottom: unset; }

.section {
  background: transparent; }

small {
  vertical-align: middle; }

svg {
  pointer-events: none; }
